fn gdc(a, b) {
  while(b!=0) {
    t = b;
    b = a % b;
    a = t;
  };
  a
};
asVar = fn gdc2(a, b) {
  while(b!=0) {
    t = b;
    b = a % b;
    a = t;
  };
  a
};

fn shor(x) {
  a = 2;
  while (a < x) {
    if (gdc(a,x) == 1) {
      b = a;
      while (b < x) {
        if (gdc(b,x) == 1) {
          if (a*b % x == 1) {
            return 'p=' + a + ' q=' + b;
          };
        };
        b = b + 1;
      };
    };
    a = a + 1;
  };
  ''
};

